#include "holberton.h"

/**
 * jack_bauer - print second to second
 *
 *
 * Return: 0
 **/


void jack_bauer(void)
{
  int a = 48, b = 48, c = 48, d = 48;

  while (a > 47 && a < 51)
    {
      while (b > 47 && b < 52)
	{
	  while (b > 47 && c < 54)
	    {
	      while (d > 47 && d < 58)
		{
		  _putchar(a);
		  _putchar(b);
		  _putchar(58);
		  _putchar(c);
		  _putchar(d);
		  _putchar('\n');
		  d++;
		}
	      c++;
	      d = 48;
	    }
	  b++;
	  c = 48;
	}
      a++;
      b = 48;
    }


}
